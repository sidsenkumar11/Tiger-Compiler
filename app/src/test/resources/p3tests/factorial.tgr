let
    var a, b, c : int;
    var d, e, f : float;
    var in1 : int;
    var in2 : float;
    func fact ( n : int ) : int begin
        if n = 0 then
            return 1;
        endif;
        if n = 1 then
            return 1;
        endif;
        a := fact(n - 1);
        return n * a;
    end;
in
    in1 := readi();
    c := fact(in1);
    printi(c);
end
